<div class='admin_content'>
<% cms_form_for :jobs_list, @jobs_list,:vertical => true, :html => { :class => 'admin_form' } do |f| -%>

<%= f.header 'Jobs List Information' %>
<%= f.text_field :name, :vertical => true, :required => true %>
<%= f.text_area :description, :vertical => true %>
<%= f.select :content_filter, JobsList::JobsListJobsList.filter_options, :vertical => true %>
<%= f.filemanager_folder :folder_id, :vertical => true %>
<%= f.submit_tag 'Create Jobs List', :vertical => true%>
<% end -%>
</div>
